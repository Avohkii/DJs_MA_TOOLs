{
  "name": "mstexplorer",
  "version": "0.0.1",
  "description": "A Metal Arms .MST file explorer",
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ]
  },
  "scripts": {
    "watch-js": "watchify js/main.js -t babelify -o js/bundle.js -v",
    "build-js": "browserify js/main.js -t babelify -o js/bundle.js -v",

    "dev": "npm run watch-js",
    "build": "npm run build-js"
  },
  "dependencies": {
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "babelify": "^7.3.0",
    "browserify": "^13.0.1",
    "watchify": "^3.7.0"
  }
}
